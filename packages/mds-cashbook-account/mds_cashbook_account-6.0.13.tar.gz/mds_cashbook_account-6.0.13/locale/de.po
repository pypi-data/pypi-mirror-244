msgctxt "model:ir.message,text:msg_category_account_unique"
msgid "The account is already in use for a category."
msgstr "Das Konto wird bereits für eine Kategorie verwendet."

msgctxt "model:ir.message,text:msg_line_missing_category_account"
msgid "There is no account associated with the '%(catname)s' category."
msgstr "Der Kategorie '%(catname)s' ist kein Konto zugeordnet."

msgctxt "model:ir.message,text:msg_line_missing_book_account"
msgid "There is no account associated with the '%(bookname)s' cashbook."
msgstr "Dem Kassenbuch '%(bookname)s' ist kein Konto zugeordnet."

msgctxt "model:ir.message,text:msg_line_missing_tax"
msgid "There is no tax associated with the '%(catname)s' category."
msgstr "Der Kategorie '%(catname)s' ist keine Steuer zugeordnet."

msgctxt "model:ir.message,text:msg_line_amount_none"
msgid "The amount for '%(taxname)s' in the line '%(linename)s' cannot be empty."
msgstr "Der Betrag für '%(taxname)s' in der Zeile '%(linename)s' kann nicht leer sein."

msgctxt "model:ir.message,text:msg_line_tax_tax0"
msgid "Tax Null"
msgstr "Steuerfrei"

msgctxt "model:ir.message,text:msg_line_tax_tax1"
msgid "Tax Half"
msgstr "Steuer halb"

msgctxt "model:ir.message,text:msg_line_tax_tax2"
msgid "Tax Full"
msgstr "Steuer voll"

msgctxt "model:ir.message,text:msg_line_netamount"
msgid "Net"
msgstr "Netto"

msgctxt "model:ir.message,text:msg_line_total_mismatch"
msgid "In the line '%(linename)s' is the sum of taxes and net is not equal to the gross amount."
msgstr "In der Zeile '%(linename)s' ist die Summe von Steuern und Netto ist ungleich dem Bruttobetrag."

msgctxt "model:ir.message,text:msg_line_tax_must_disabled"
msgid "In the line '%(linename)s' the fields for taxes must be inactive for the current bookingtype."
msgstr "Die Felder für Steuern müssen in der Zeile '%(linename)s' beim aktuellen Buchungstyp inaktiv sein."

msgctxt "model:ir.message,text:msg_line_party_required"
msgid "For the line '%(recname)s' it is necessary to specify a party."
msgstr "Für die Zeile '%(recname)s' ist die Angabe einer Partei notwendig."


